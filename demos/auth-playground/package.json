{
  "name": "demo-auth-playground",
  "description": "Identity Vault and Auth Connect Playground",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "author": "Ionic Framework",
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "vue-tsc && vite build && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "^6.0.1",
    "@capacitor/core": "^6.1.2",
    "@capacitor/haptics": "^6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/keyboard": "^6.0.2",
    "@capacitor/preferences": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "@ionic-enterprise/auth": "~6.0.2",
    "@ionic-enterprise/identity-vault": "~5.14.4",
    "@ionic/vue": "~8.3.2",
    "@ionic/vue-router": "~8.3.2",
    "axios": "~1.7.7",
    "ionicons": "^7.4.0",
    "vee-validate": "~4.13.2",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5",
    "yup": "~1.4.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "~2.1.2",
    "@vue/eslint-config-typescript": "~13.0.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "~13.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "~3.5.0",
    "eslint-plugin-vue": "~9.28.0",
    "jsdom": "~25.0.1",
    "typescript": "~5.6.2",
    "vite": "^5.4.8",
    "vitest": "~2.1.2",
    "vue-tsc": "^2.1.6",
    "wait-for-expect": "~3.0.2"
  }
}
