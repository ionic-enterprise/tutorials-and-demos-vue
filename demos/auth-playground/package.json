{
  "name": "demo-auth-playground",
  "description": "Identity Vault and Auth Connect Playground",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "author": "Ionic Framework",
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "vue-tsc && vite build && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^7.3.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.3.0",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.3.0",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/privacy-screen": "^1.1.0",
    "@capacitor/status-bar": "^7.0.1",
    "@ionic-enterprise/auth": "~7.1.1",
    "@ionic-enterprise/identity-vault": "^6.0.2",
    "@ionic/vue": "^8.6.0",
    "@ionic/vue-router": "^8.6.0",
    "axios": "^1.9.0",
    "ionicons": "^8.0.9",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.3.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.10",
    "wait-for-expect": "^3.0.2"
  }
}
