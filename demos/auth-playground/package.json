{
  "name": "demo-auth-playground",
  "description": "Identity Vault and Auth Connect Playground",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "author": "Ionic Framework",
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "vue-tsc && vite build && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.6.0",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "^5.0.7",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@ionic-enterprise/auth": "~5.1.3",
    "@ionic-enterprise/identity-vault": "~5.12.2",
    "@ionic/vue": "~7.6.5",
    "@ionic/vue-router": "~7.6.5",
    "@vitest/coverage-v8": "~1.2.1",
    "axios": "~1.6.5",
    "ionicons": "^7.2.2",
    "vee-validate": "~4.12.4",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "yup": "~1.3.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "~12.0.0",
    "@vue/test-utils": "^2.4.3",
    "cypress": "~13.6.3",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "~2.15.1",
    "eslint-plugin-vue": "~9.20.1",
    "jsdom": "~23.2.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.11",
    "vitest": "~1.2.1",
    "vue-tsc": "^1.8.27",
    "wait-for-expect": "~3.0.2"
  }
}
