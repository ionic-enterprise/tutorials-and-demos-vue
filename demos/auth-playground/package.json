{
  "name": "demo-auth-playground",
  "description": "Identity Vault and Auth Connect Playground",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "author": "Ionic Framework",
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "vue-tsc && vite build && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.1.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.1.0",
    "@capacitor/keyboard": "^6.0.1",
    "@capacitor/preferences": "^6.0.1",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic-enterprise/auth": "~6.0.0",
    "@ionic-enterprise/identity-vault": "~5.14.2",
    "@ionic/vue": "~8.2.4",
    "@ionic/vue-router": "~8.2.4",
    "axios": "~1.7.2",
    "ionicons": "^7.4.0",
    "vee-validate": "~4.13.1",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "yup": "~1.4.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "~1.6.0",
    "@vue/eslint-config-typescript": "~13.0.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "~13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "~3.3.0",
    "eslint-plugin-vue": "~9.26.0",
    "jsdom": "~24.1.0",
    "typescript": "~5.5.2",
    "vite": "^5.3.2",
    "vitest": "~1.6.0",
    "vue-tsc": "^2.0.24",
    "wait-for-expect": "~3.0.2"
  }
}
