{
  "name": "demo-security-trifecta",
  "description": "Security Trifecta",
  "author": {
    "name": "The Ionic Customer Success Team",
    "email": "support@ionic.io"
  },
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic-enterprise/auth": "~6.0.0",
    "@ionic-enterprise/identity-vault": "~5.14.0",
    "@ionic-enterprise/secure-storage": "^3.0.3",
    "@ionic/vue": "~8.1.0",
    "@ionic/vue-router": "~8.1.0",
    "axios": "~1.6.8",
    "core-js": "^3.37.0",
    "ionicons": "^7.4.0",
    "vee-validate": "^4.12.7",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.5",
    "@capacitor/cli": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "~1.6.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^13.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "~3.2.0",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "~24.0.0",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vitest": "~1.6.0",
    "vue-tsc": "^2.0.16",
    "wait-for-expect": "~3.0.2"
  }
}
