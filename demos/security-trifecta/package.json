{
  "name": "demo-security-trifecta",
  "description": "Security Trifecta",
  "author": {
    "name": "The Ionic Customer Success Team",
    "email": "support@ionic.io"
  },
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.4",
    "@capacitor/app": "^5.0.7",
    "@capacitor/core": "^5.7.4",
    "@capacitor/haptics": "^5.0.7",
    "@capacitor/ios": "^5.7.4",
    "@capacitor/keyboard": "^5.0.8",
    "@capacitor/splash-screen": "^5.0.7",
    "@capacitor/status-bar": "^5.0.7",
    "@ionic-enterprise/auth": "~5.1.3",
    "@ionic-enterprise/identity-vault": "~5.12.4",
    "@ionic-enterprise/secure-storage": "^3.0.3",
    "@ionic/vue": "~7.8.2",
    "@ionic/vue-router": "~7.8.2",
    "axios": "~1.6.8",
    "core-js": "^3.36.1",
    "ionicons": "^7.3.1",
    "vee-validate": "^4.12.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.4",
    "@capacitor/cli": "^5.7.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "~1.4.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "cypress": "^13.7.1",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "~2.15.1",
    "eslint-plugin-vue": "^9.24.0",
    "jsdom": "~24.0.0",
    "typescript": "~5.4.3",
    "vite": "^5.2.6",
    "vitest": "~1.4.0",
    "vue-tsc": "^2.0.7",
    "wait-for-expect": "~3.0.2"
  }
}
