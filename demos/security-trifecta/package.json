    "@capacitor/ios": "^7.2.0",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/privacy-screen": "^1.1.0",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@ionic-enterprise/auth": "^7.1.1",
    "@ionic-enterprise/identity-vault": "^6.0.2",
    "@ionic-enterprise/secure-storage": "^3.0.6",
    "@ionic/vue": "^8.5.7",
    "@ionic/vue-router": "^8.5.7",
    "axios": "^1.9.0",
    "core-js": "^3.42.0",
    "ionicons": "^8.0.8",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1",
    "yup": "^1.6.1"
