{
  "name": "security-trifecta",
  "description": "Security Trifecta",
  "author": {
    "name": "The Ionic Customer Success Team",
    "email": "support@ionic.io"
  },
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.5.1",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@ionic-enterprise/auth": "~5.1.1",
    "@ionic-enterprise/identity-vault": "~5.12.1",
    "@ionic-enterprise/secure-storage": "^3.0.2",
    "@ionic/vue": "~7.5.7",
    "@ionic/vue-router": "~7.5.7",
    "axios": "~1.6.2",
    "core-js": "^3.33.3",
    "ionicons": "^7.2.1",
    "vee-validate": "^4.12.2",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.1",
    "@capacitor/cli": "^5.5.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "~0.34.6",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "cypress": "^13.6.0",
    "eslint": "^8.54.0",
    "eslint-plugin-cypress": "~2.15.1",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "~23.0.1",
    "typescript": "~5.3.2",
    "vite": "^5.0.4",
    "vitest": "~0.34.6",
    "vue-tsc": "^1.8.24",
    "wait-for-expect": "~3.0.2"
  }
}
