{
  "name": "demo-iv-ac",
  "description": "Using IV and AC in an Ionic app",
  "author": {
    "name": "Ionic Customer Success Team",
    "email": "support@ionic.io",
    "url": "https://ionic.io"
  },
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/app": "^7.0.0",
    "@capacitor/core": "^7.0.1",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/ios": "^7.0.1",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@capacitor/share": "^7.0.0",
    "@capacitor/splash-screen": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@ionic-enterprise/auth": "^7.0.1",
    "@ionic-enterprise/identity-vault": "^5.14.5",
    "@ionic/vue": "^8.4.3",
    "@ionic/vue-router": "^8.4.3",
    "axios": "^1.7.9",
    "ionicons": "^7.4.0",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.4",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.19.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.2.0",
    "wait-for-expect": "^3.0.2"
  }
}
