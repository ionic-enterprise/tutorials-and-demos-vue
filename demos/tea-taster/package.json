    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "cypress": "^13.4.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "~22.1.0",
    "typescript": "~5.2.2",
    "vite": "^4.5.0",
    "vitest": "~0.34.6",
    "vue-tsc": "^1.8.22",
    "wait-for-expect": "~3.0.2"
