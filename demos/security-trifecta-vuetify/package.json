    "@vitejs/plugin-vue": "~5.1.2",
    "@vitest/coverage-v8": "~2.0.5",
    "@vue/eslint-config-typescript": "~13.0.0",
    "@vue/test-utils": "~2.4.6",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "~3.5.0",
    "eslint-plugin-vue": "^9.27.0",
    "typescript": "~5.5.4",
    "vite": "~5.4.0",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "~2.0.5",
    "vue-tsc": "^2.0.29",
    "wait-for-expect": "~3.0.2"
  }
