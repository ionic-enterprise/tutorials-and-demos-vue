    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/splash-screen": "^6.0.2",
    "@ionic-enterprise/auth": "~6.0.2",
    "@ionic-enterprise/identity-vault": "~5.14.4",
    "@ionic-enterprise/secure-storage": "~3.0.3",
    "@mdi/font": "~7.4.47",
    "@vue/test-utils": "~2.4.6",
    "axios": "~1.7.7",
    "jsdom": "~25.0.1",
    "roboto-fontface": "^0.10.0",
    "vee-validate": "~4.13.2",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.2",
    "webfontloader": "^1.6.28",
    "yup": "~1.4.0"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.5",
