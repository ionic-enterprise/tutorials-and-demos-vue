    "@vitejs/plugin-vue": "~5.1.4",
    "@vitest/coverage-v8": "~2.1.3",
    "@vue/eslint-config-typescript": "~13.0.0",
    "@vue/test-utils": "~2.4.6",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "~3.5.0",
    "eslint-plugin-vue": "^9.29.0",
    "typescript": "~5.6.3",
    "vite": "~5.4.9",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "~2.1.3",
    "vue-tsc": "^2.1.6",
    "wait-for-expect": "~3.0.2"
  }
