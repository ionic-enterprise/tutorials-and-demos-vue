    "@ionic-enterprise/identity-vault": "^5.14.5",
    "@ionic-enterprise/secure-storage": "^3.0.4",
    "@mdi/font": "^7.4.47",
    "@vue/test-utils": "^2.4.6",
    "axios": "^1.7.9",
    "roboto-fontface": "^0.10.0",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.6",
