    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^3.0.2",
    "vue-tsc": "^2.2.0",
    "wait-for-expect": "^3.0.2"
