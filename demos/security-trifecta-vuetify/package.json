{
  "name": "security-trifecta-vuetify",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#fafafa' --splashBackgroundColor '#fafafa' --iconBackgroundColorDark '#646464' --splashBackgroundColorDark '#646464'",
    "build": "vue-tsc --noEmit && vite build && cap sync",
    "dev": "vite",
    "ios": "cap open ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/splash-screen": "^5.0.6",
    "@ionic-enterprise/auth": "~5.1.1",
    "@ionic-enterprise/identity-vault": "~5.12.1",
    "@ionic-enterprise/secure-storage": "~3.0.2",
    "@mdi/font": "~7.3.67",
    "@vitest/coverage-v8": "~0.34.6",
    "@vue/test-utils": "~2.4.3",
    "axios": "~1.6.2",
    "jsdom": "~23.0.1",
    "roboto-fontface": "^0.10.0",
    "vee-validate": "~4.12.2",
    "vitest": "~0.34.6",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.4",
    "webfontloader": "^1.6.28",
    "yup": "~1.3.2"
  },
  "devDependencies": {
    "@capacitor/assets": "~3.0.1",
    "@capacitor/cli": "^5.5.1",
    "@types/node": "~20.10.1",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "~4.5.0",
    "@vue/eslint-config-typescript": "~12.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-cypress": "~2.15.1",
    "eslint-plugin-vue": "^9.19.2",
    "typescript": "~5.3.2",
    "vite": "~5.0.4",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.8.24",
    "wait-for-expect": "~3.0.2"
  }
}
